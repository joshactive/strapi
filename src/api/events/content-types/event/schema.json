{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "wpId": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "dateFrom": {
      "type": "date",
      "required": true
    },
    "dateUntil": {
      "type": "date",
      "required": true
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "singleOccupancyPriceEvent": {
      "type": "decimal"
    },
    "productLink": {
      "type": "string"
    },
    "venueLink": {
      "type": "string"
    },
    "bookingLink": {
      "type": "string"
    },
    "boardBasisEvent": {
      "type": "string"
    },
    "dateText": {
      "type": "string"
    },
    "product": {
      "type": "string"
    },
    "countryEvents": {
      "type": "string"
    },
    "buttonColour": {
      "type": "string"
    },
    "isSoldOut": {
      "type": "boolean",
      "default": false
    },
    "buttonText": {
      "type": "string"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}

