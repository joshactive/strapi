{
  "kind": "collectionType",
  "collectionName": "redirects",
  "info": {
    "singularName": "redirect",
    "pluralName": "redirects",
    "displayName": "Redirect",
    "description": "URL redirects managed in Strapi"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "sourcePath": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "destinationPath": {
      "type": "string",
      "required": true
    },
    "statusCode": {
      "type": "enumeration",
      "enum": [
        "301",
        "302"
      ],
      "default": "302",
      "required": true
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "notes": {
      "type": "text"
    }
  }
}

