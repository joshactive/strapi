{
  "kind": "collectionType",
  "collectionName": "sitemap_patterns",
  "info": {
    "singularName": "sitemap-pattern",
    "pluralName": "sitemap-patterns",
    "displayName": "Sitemap Pattern",
    "description": "Define URL patterns for content types"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "collectionName": {
      "type": "enumeration",
      "required": true,
      "unique": true,
      "enum": [
        "api::pickleball-holiday.pickleball-holiday",
        "api::blog.blog",
        "api::event.event",
        "api::tennis-holiday.tennis-holiday",
        "api::tennis-academy.tennis-academy",
        "api::tennis-clinic.tennis-clinic",
        "api::padel-tennis-holiday.padel-tennis-holiday",
        "api::ski-holiday.ski-holiday",
        "api::review.review",
        "api::team.team"
      ],
      "default": "api::blog.blog"
    },
    "pattern": {
      "type": "string",
      "required": true,
      "default": "/[slug]"
    },
    "priority": {
      "type": "decimal",
      "min": 0,
      "max": 1,
      "default": 0.5
    },
    "changeFrequency": {
      "type": "enumeration",
      "enum": [
        "always",
        "hourly",
        "daily",
        "weekly",
        "monthly",
        "yearly",
        "never"
      ],
      "default": "monthly"
    }
  }
}
