{
    "$schema": "https://backboard.railway.app/railway.schema.json",
    "build": {
        "builder": "RAILPACK"
    },
    "deploy": {
        "healthcheckPath": "/api/healthcheck",
        "healthcheckTimeout": 300,
        "startCommand": "npm run start",
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 10
    }
}
